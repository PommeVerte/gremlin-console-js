{
  "name": "gremlin-console-js",
  "version": "0.0.0",
  "description": "Console component for gremlin-server",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src -d lib",
    "build:docs": "mr-doc --source src --output docs --name gremlin-console -p --theme mr-doc-theme-cayman",
    "build:umd": "NODE_ENV=production webpack src/index.js umd/gremlin-console.js && cp -r static/* umd/",
    "build:min": "NODE_ENV=production webpack -p src/index.js umd/gremlin-console.min.js",
    "build:watch": "npm run build -- --watch"
  },
  "dependencies": {
    "babel-cli": "~6.6.5",
    "babel-core": "~6.7.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-2": "~6.5.0",
    "babel-register": "~6.7.2",
    "cytoscape": "~2.6.6",
    "events": "^1.1.0",
    "gremlin": "jbmusso/gremlin-javascript",
    "jquery": "~2.2.1",
    "jquery-ui": "~1.10.5",
    "webpack": "~1.12.14"
  },
  "devDependencies": {
    "mr-doc": "^3.2.1",
    "mr-doc-theme-cayman": "^1.4.0"
  },
  "keywords": [
    "gremlin",
    "console"
  ],
  "author": "Dylan Millikin",
  "license": "Apache2"
}
